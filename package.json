{
  "name": "gatsby-eui-starter",
  "private": true,
  "description": "Start building Kibana protoypes quickly with the Gatsby EUI Starter",
  "version": "1.0.0",
  "author": "Davey Holler <davey.holler@elastic.co>",
  "dependencies": {
    "@elastic/datemath": "^5.0.2",
    "@elastic/eui": "^13.1.1",
    "@svgr/cli": "^4.3.3",
    "@types/styled-components": "^4.1.19",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "framer-motion": "^1.6.15",
    "gatsby": "^2.32.13",
    "gatsby-image": "^2.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-typescript": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-sharp": "^2.1.19",
    "gh-pages": "^2.0.1",
    "mem": "^4.0.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.2",
    "@types/node": "^12.0.2",
    "@types/react-dom": "^16.8.4",
    "prettier": "^1.17.1",
    "typescript": "^3.3.3"
  },
  "keywords": [
    "gatsby",
    "kibana",
    "eui",
    "elastic"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:types && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "svg-to-jsx": "svgr < svgr/illustration-graph-steps.svg > svgr/graph-illustration.js --svgo-config svgr/svgo.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/gatsby-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/gatsby-eui-starter/issues"
  }
}
